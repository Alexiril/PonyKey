  "GameObjects": {
    "LoadingSpinner": {
      "Engine.BaseComponents.Transform": {
        "Position": {
          "@Vector": {
            "X": "@centerX",
            "Y": "@centerY"
          }
        }
      },
      "Engine.BaseComponents.Sprite": {
        "Texture": {
          "@Asset": {
            "asset": "SVG",
            "type": "Texture",
            "assetName": "loadingSpinner",
            "size": {
              "@Vector": {
                "X": {
                  "@Multiply": [
                    "@sizeX",
                    "0.7"
                  ]
                },
                "Y": {
                  "@Multiply": [
                    "@sizeX",
                    "0.7"
                  ]
                }
              }
            }
          }
        }
      },
      "Engine.BaseComponents.LoadingSpinner": {
        "Speed": {
          "@FloatValue": "5"
        }
      },
      "Game.Components.MainMenu.MainMenuInit": {}
    },
    "Background": {
      "Engine.BaseComponents.Transform": {
        "Position": {
          "@Vector": {
            "X": "@centerX",
            "Y": "@centerY"
          }
        }
