{
  "Name": "Main menu",
  "BackgroundColor": {
    "@Color": "DeepSkyBlue"
  },
  "Hierarchy": [
    "LoadingSpinner",
    "Background",
    "Logo",
    "PlayButton",
    "ExitButton",
    "DisplayButton",
    "SoundButton",
    "InfoButton"
