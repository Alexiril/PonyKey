            "assetName": "MainMenu/Background",
            "size": {
              "@Vector": {
                "X": {
                  "@FloatValue": "@sizeX"
                },
                "Y": {
                  "@FloatValue": "@sizeY"
                }
              }
            }
          }
        }
      },
      "Engine.BaseComponents.SoundSource": {
        "Sound": {
          "@Asset": {
            "asset": "Content",
            "type": "SoundEffect",
            "filename": "MainMenu/BackgroundMusicMenu"
          }
        },
        "IsLooped": {
          "@BoolValue": "true"
        },
        "Volume": {
          "@PlayerSettings": {
            "value": "vl",
            "default": {
              "@FloatValue": "1"
            }
          }
        },
        "PlayAtStart": {
          "@BoolValue": "true"
        }
      }
    },
    "Logo": {
